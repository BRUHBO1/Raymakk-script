local humanoid = player.Character.Humanoid
local character = player.Character or player.CharacterAdded:Wait()
local UserInputService = game:GetService("UserInputService")

local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://12502664044"

local playAnim = humanoid:LoadAnimation(anim)
playAnim:Play()

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0

wait(0.25)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 50

wait(0.75)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 5

local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://12509505723"

local playAnim = humanoid:LoadAnimation(anim)
playAnim:Play()

wait(0.5)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 0

local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://71060716968719"

local playAnim = humanoid:LoadAnimation(anim)
playAnim:Play()
wait(2.5)
playAnim:Stop()
local anim = Instance.new("Animation")
anim.AnimationId = "rbxassetid://105811521074269"

local playAnim = humanoid:LoadAnimation(anim)
playAnim:Play()

wait(1)

game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
end)

